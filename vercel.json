{
    "version": 2,
    "env": {
        "GITHUB_TOKEN": "@github_token",
        "ENCRYPTION_KEY": "@encryption_key"
    },
    "routes": [
        {
            "src": "/api/transcripts",
            "methods": ["GET", "OPTIONS"],
            "dest": "/api/transcripts-secure.js"
        },
        {
            "src": "/api/key",
            "methods": ["GET", "OPTIONS"],
            "dest": "/api/key.js"
        },
        {
            "src": "/api/auth",
            "methods": ["POST", "OPTIONS"],
            "dest": "/api/auth.js"
        }
    ]
} 